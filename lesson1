for f in range(7):
    if f == 0:
        print("今天我吃了：鸡腿饭")
    elif f == 1:
        print("今天我吃了：鸭腿饭")
    elif f == 2:
        print("今天我吃了：鹅腿饭")
    elif f == 3:
        print("今天我吃了：麻辣烫")
    elif f == 4:
         print("今天我吃了：凉皮")
    elif f == 5:
        print("今天我吃了：烧烤")
    else :
        print("今天我吃了：沙拉")
